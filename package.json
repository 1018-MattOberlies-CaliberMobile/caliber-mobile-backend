{
  "name": "caliber-mobile-backend",
  "version": "1.0.0",
  "description": "Serverless aws-nodejs-typescript template",
  "main": "serverless.ts",
  "scripts": {
    "test": "jest",
    "db:create": "ts-node rds-set-up/create.ts",
    "db:drop": "ts-node rds-set-up/drop.ts",
    "db:reset": "npm run db:drop && npm run db:create",
    "sls:info": "sls info --verbose | tee ./.slsinfo",
    "sls:deploy": "sls deploy && npm run sls:info"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "dependencies": {
    "@middy/core": "^1.5.2",
    "@middy/http-json-body-parser": "^1.5.2",
    "babel-jest": "^27.0.6",
    "pg": "^8.7.1",
    "source-map-support": "^0.5.19",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.6.5",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@serverless/typescript": "^2.52.1",
    "@types/aws-lambda": "^8.10.71",
    "@types/jest": "^26.0.24",
    "@types/node": "^14.17.6",
    "@types/pg": "^8.6.1",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "dotenv": "^10.0.0",
    "esbuild": "^0.12.16",
    "eslint": "^7.31.0",
    "eslint-config-airbnb-base": "^14.2.1",
